{
  "name": "s-bridge",
  "version": "1.0.13-0",
  "description": "基于dsBridge扩展的库",
  "source": "src/index.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionuv/s-bridge.git"
  },
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir ./dist",
    "copy:ts": "cp src/index.js dist/index.ts",
    "build:d.ts": "tsc --declaration dist/index.ts",
    "prepublish": "concurrently \"npm run build\" \"npm run copy:ts\" \"npm run build:d.ts\""
  },
  "keywords": [
    "dsBridge",
    "ios",
    "android"
  ],
  "author": {
    "name": "zlzkyuxin",
    "email": "1134548344@qq.com",
    "url": "https://github.com/zlzkyuxin/"
  },
  "license": "ISC",
  "dependencies": {
    "dsbridge": "^3.1.4"
  },
  "devDependencies": {
    "microbundle": "^0.12.3",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
