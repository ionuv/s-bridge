{
  "name": "s-bridge",
  "version": "1.0.15-0",
  "description": "基于dsBridge扩展的库",
  "source": "src/index.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionuv/s-bridge.git"
  },
  "scripts": {
    "build": "webpack",
    "copy:ts": "cp src/index.js dist/index.ts",
    "build:d.ts": "tsc --declaration dist/index.ts",
    "remove:ts": "rm -f dist/index.ts",
    "prepublish": "npm run copy:ts && npm run build:d.ts && npm run remove:ts && npm run build"
  },
  "keywords": [
    "dsBridge",
    "ios",
    "android"
  ],
  "author": {
    "name": "zlzkyuxin",
    "email": "1134548344@qq.com",
    "url": "https://github.com/zlzkyuxin/"
  },
  "license": "ISC",
  "dependencies": {
    "dsbridge": "^3.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "microbundle": "^0.12.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
